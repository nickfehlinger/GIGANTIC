<p id="notice"><%= notice %></p>

<h1>Events</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Company</th>
      <th>Street address</th>
      <th>City</th>
      <th>State</th>
      <th>Uniform</th>
      <th>Start time</th>
      <th>End time</th>
      <th>Pay rate</th>
      <th>Pay type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.title %></td>
        <td><%= event.description %></td>
        <td><%= event.company %></td>
        <td><%= event.street_address %></td>
        <td><%= event.city %></td>
        <td><%= event.state %></td>
        <td><%= event.uniform %></td>
        <td><%= event.start_time %></td>
        <td><%= event.end_time %></td>
        <td><%= event.pay_rate %></td>
        <td><%= event.pay_type %></td>
        <td><%= link_to 'Show', event %></td>
        <td><%= link_to 'Edit', edit_event_path(event) %></td>
        <td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
      <div id="calendar"></div>
      <script type="text/javascript">$('#calendar').fullCalendar(
        {events: 'events.json'}
      );</script>
<br>

<%= link_to 'New Event', new_event_path %>
